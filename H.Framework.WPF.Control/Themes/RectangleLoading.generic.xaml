    <Storyboard x:Key="TextColorStoryBoard" RepeatBehavior="Forever">
        <ColorAnimation
            AutoReverse="True"
            BeginTime="0:0:0"
            Storyboard.TargetName="LoadText"
            Storyboard.TargetProperty="Foreground.Color"
            From="#FF9666C5"
            To="Transparent" />
    </Storyboard>
    <Storyboard x:Key="TextStoryBoard" RepeatBehavior="Forever">
        <StringAnimationUsingKeyFrames
            BeginTime="00:00:00"
            Storyboard.TargetName="LoadText"
            Storyboard.TargetProperty="Text">
            <DiscreteStringKeyFrame KeyTime="00:00:00" Value="Loading" />
            <DiscreteStringKeyFrame KeyTime="00:00:00.5" Value="Loading." />
            <DiscreteStringKeyFrame KeyTime="00:00:01" Value="Loading.." />
            <DiscreteStringKeyFrame KeyTime="00:00:01.5" Value="Loading..." />
            <DiscreteStringKeyFrame KeyTime="00:00:02" Value="Loading...." />
            <DiscreteStringKeyFrame KeyTime="00:00:02.5" Value="Loading....." />
        </StringAnimationUsingKeyFrames>
    </Storyboard>
